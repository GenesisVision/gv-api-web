{
  "name": "gv-api-web",
  "version": "1.0.215",
  "description": "genesis.vision-api-web",
  "license": "Unlicense",
  "main": "distribution/index.js",
  "types": "distribution/types.d.ts",
  "scripts": {
    "generate-tsd": "./bash/replace.sh && jsdoc -t node_modules/tsd-jsdoc/dist -r ./src -d ./distribution && ./bash/add_exports.sh",
    "build": "babel src --out-dir distribution && npm run generate-tsd",
    "precommit": "npm install && npm run build && npm run addfiles",
    "addfiles": "git add .",
    "test": "mocha --compilers js:babel-core/register --recursive"
  },
  "browser": {
    "fs": false
  },
  "dependencies": {
    "superagent": "3.5.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "expect.js": "~0.3.1",
    "husky": "^0.14.3",
    "jsdoc": "^3.5.5",
    "mocha": "~2.3.4",
    "sinon": "1.17.3",
    "tsd-jsdoc": "^2.0.4"
  }
}
