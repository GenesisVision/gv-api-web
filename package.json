    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "expect.js": "~0.3.1",
    "jsdoc": "3.5.5",
    "mocha": "^5.2.0",
    "shelljs": "^0.8.3",
    "sinon": "1.17.3",
    "tsd-jsdoc": "2.0.4"
